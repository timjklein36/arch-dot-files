    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [" "],
            "commands": [
                "workbench.action.showCommands",
            ]
        }
    ],
    "vim.use<C-r>": "true",
    "vim.useSystemClipboard": true,
    "vim.easymotion": true,
    "vim.foldfix": true,
    "tslint.configFile": "tslint.json"
}
