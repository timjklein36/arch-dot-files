    "cSpell.ignoreRegExpList": [
        "r'[A-Z]+'",
    ],
    "window.menuBarVisibility": "toggle",
    "python.pythonPath": "~/.virtualenvs/${workspaceFolderBasename}/bin/python",
    "files.watcherExclude": {
        "**/venv/**": true,
        "**/__pycache__": true,
        "**/*.pyc": true,
        "**/node_modules": true
    },
    "gitlens.blame.avatars": false,
    "gitlens.blame.highlight.enabled": false,
    "gitlens.codeLens.authors.enabled": false,
    "gitlens.currentLine.enabled": false,
    "gitlens.liveshare.allowGuestAccess": false,
    "gitlens.codeLens.recentChange.enabled": false,
    "gitlens.blame.format": "${author|10} ${message|30?} ${agoOrDate|14-}",
    "files.exclude": {
        "**/venv/**": true,
        "**/__pycache__": true,
        "**/*.pyc": true,
        "**/node_modules": true
    },
    "window.zoomLevel": 0,
    "workbench.startupEditor": "newUntitledFile",
    "python.linting.pycodestyleEnabled": true,
    "python.linting.pycodestylePath": "~/.virtualenvs/${workspaceFolderBasename}/bin/pycodestyle",
    "python.linting.pycodestyleArgs": [
        "--ignore=E402,W504"
    ],
    "python.linting.pylintPath": "~/.virtualenvs/${workspaceFolderBasename}/bin/pylint",
    "python.linting.pylintArgs": [
        "--load-plugins",
        "pylint_django"
    ],
    "python.linting.pycodestyleCategorySeverity.E": "Warning",
    "files.autoSave": "onFocusChange",
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.colorTheme": "Banner (rainglow)",
    "terminal.integrated.fontSize": 13,
    "workbench.colorCustomizations": {
        "editorHint.border": "#dadada",
        "editorHint.foreground": "#dadada00",
        "[Banner (rainglow)]": {
            "activityBar.foreground": "#ad4eca",
            "activityBar.activeBackground": "#170a2e",
            "activityBar.activeBorder": "#7cd827",
            "statusBar.background": "#7cd827",
            "statusBar.foreground": "#260035",
            "badge.foreground": "#260035",
            "activityBarBadge.foreground": "#260035",
            "extensionBadge.remoteForeground": "#260035",
            "editorHint.border": "#eb9dff",
            "editorHint.foreground": "#fdfdfd00",
            "editorInfo.foreground": "#d0dfc3",
            "editorWarning.foreground": "#9cf339",
            "editorError.foreground": "#ff0077"
        }
    },
    "editor.tokenColorCustomizations": {
        "[Darcula Theme from IntelliJ]": {
            "textMateRules": [
                {
                    "scope": "variable.parameter.function-call.python",
                    "settings": {
                        "foreground": "#eb6d6d"
                    }
                },
                {
                    "scope": "keyword.codetag.notation.python",
                    "settings": {
                        "foreground": "#bde700",
                        "fontStyle": "bold"
                    }
                }
            ]
        },
        "[Banner (rainglow)]": {
            "textMateRules":[
                {
                    "scope": "",
                    "settings": {
                        "foreground": "#a6a0ff"
                    }
                },
                {
                    "scope": "variable.language.this.ts, storage.modifier.ts",
                    "settings": {
                        "foreground": "#e7ddff"
                    }
                },
                {
                    "scope": "meta.object-literal.key.ts, meta.item-access.arguments.python",
                    "settings": {
                        "foreground": "#ffffff"
                    }
                },
                {
                    "scope": "meta.method.declaration.ts meta.return.type.ts entity.name.type.ts",
                    "settings": {
                        "foreground": "#1ec010"
                    }
                },
                {
                    "scope": "meta.type.annotation.ts entity.name.type.ts",
                    "settings": {
                        "foreground": "#1ec010"
                    }
                },
                {
                    "scope": "entity.other.inherited-class.ts",
                    "settings": {
                        "foreground": "#b063f0"
                    }
                },
                {
                    "scope": "comment.block.documentation.ts",
                    "settings": {
                        "foreground": "#71649b"
                    }
                },
                {
                    "scope": "support.type.property-name.json.comments",
                    "settings": {
                        "foreground": "#b063f0"
                    }
                },
                {
                    "scope": "meta.function-call.generic.python, entity.name.function.ts, new.expr.ts entity.name.type.ts",
                    "settings": {
                        "foreground": "#76c7ae"
                    }
                },
                {
                    "scope": "source.python, source.ts, variable.other.property.ts, variable.other.object.property.ts, variable.other.object.ts, variable.other.readwrite.ts",
                    "settings": {
                        "foreground": "#e3bbfa"
                    }
                },
                {
                    "scope": "entity.name.function.python, meta.definition.method.ts entity.name.function.ts",
                    "settings": {
                        "foreground": "#e359ff",
                        "fontStyle": "bold"
                    }
                },
                {
                    "scope": "comment.line",
                    "settings": {
                        "foreground": "#b288c4"
                    }
                },
                {
                    "scope": "keyword.codetag.notation.python",
                    "settings": {
                        "foreground": "#88ec2b",
                    }
                },
                {
                    "scope": "punctuation.definition.decorator.python",
                    "settings": {
                        "foreground": "#00ffff"
                    }
                },
                {
                    "scope": "entity.name.function.decorator.python, meta.decorator.ts meta.function-call.ts entity.name.function.ts",
                    "settings": {
                        "foreground": "#00ffff"
                    }
                },
                {
                    "scope": "support.type.python, support.type.primitive.ts, support.function.builtin.python, support.class.builtin.ts",
                    "settings": {
                        "foreground": "#75ff68",
                    }
                },
                {
                    "scope": "constant.numeric",
                    "settings": {
                        "foreground": "#ff49f6"
                    }
                },
                {
                    "scope": "meta.function.python support.type.python",
                    "settings": {
                        "foreground": "#e359ff",
                        "fontStyle": "bold"
                    }
                },
                {
                    "scope": "keyword.control.flow.python",
                    "settings": {
                        "foreground": "#c1daab"
                    }
                }
            ]
        }
    },
    "typescript.tsserver.log": "verbose",
    "tslint.alwaysShowRuleFailuresAsWarnings": false,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "git.confirmSync": false,
    "diffEditor.ignoreTrimWhitespace": true,
    "files.associations": {
        "Dockerfile*": "dockerfile",
        "**/bin/*": "shellscript"
    },
    "problems.showCurrentInStatus": true,
    "python.formatting.autopep8Path": "~/.virtualenvs/${workspaceFolderBasename}/bin/autopep8",
    "markdown-pdf.outputDirectory": "~/Downloads/markdown-pdf",
    "cSpell.enableFileTypes": [
